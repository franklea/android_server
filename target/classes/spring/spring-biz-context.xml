<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <bean id="userService" class="com.cs.biz.impl.UserManagerImpl">
        <property name="userMapper" ref="userMapper"></property>
    </bean>
    <bean id="stationService" class="com.cs.biz.impl.StationManagerImpl">
        <property name="stationMapper" ref="stationMapper"></property>
    </bean>
    <bean id="dataService" class="com.cs.biz.impl.DataManagerImpl">
        <property name="dataMapper" ref="dataMapper"></property>
    </bean>
    <bean id="msgService" class="com.cs.biz.impl.MsgManagerImpl">
        <property name="msgMapper" ref="msgMapper"></property>
    </bean>
</beans>