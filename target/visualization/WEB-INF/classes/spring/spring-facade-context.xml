<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mvc="http://www.springframework.org/schema/mvc"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <bean id="userFacade" class="com.cs.facade.impl.UserFacadeImpl">
        <property name="userService" ref="userService"></property>
    </bean>
    <bean id="uploadFacade" class="com.cs.facade.impl.UploadFacadeImpl">   
    </bean>
    <bean id="stationFacade" class="com.cs.facade.impl.StationFacadeImpl">
    	<property name="stationService" ref="stationService"></property>   
    </bean>
    <bean id="dataFacade" class="com.cs.facade.impl.DataFacadeImpl">
    	<property name="dataService" ref="dataService"></property>   
    </bean>
    <bean id = "headerFacade" class = "com.cs.facade.impl.HeaderFacadeImpl">
    </bean>
</beans>