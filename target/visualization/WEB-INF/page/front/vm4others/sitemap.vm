<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><meta http-equiv="Cache-control" content="private"/>
<link type="text/css" rel="stylesheet" href="/css/webcss.css"/>
<title>中冶赛迪重庆信息技术有限公司</title>
</head>
<body>
<div class="main">
    #parse("header.vm")
    <div class="down_frame" style="float:left;margin-top:20px;width:980px">
        

        <div class="current_location" style="width:980px;">
            <img style="margin-left:20px;float:left;margin-top:6px;" src="/img/dangqian.png">
            <a class="location_a"  style="line-height:17px;"><span class="location_span2">网站地图</span></a>
            <div style="clear:both"></div>
        </div>
        <div style="clear:both;height:20px;"></div>
        <div class="right_item_up" style="margin-top:3px;width:980px;">
            <img style="float:left;margin-top:3px;" src="/img/twopoint_03.png">
            <a  href="" style="float:left;margin-left:5px;font-family:Microsoft YaHei,SimHei;font-size:19px;" >网站地图</a>
        </div>
        <ul class="sitemap_ul1">
        	<li class="sitemap_li1">
	        	<a href="/solution/index/1" class="sitemap_title1">行业解决方案</a>
	        	<div style="clear:both"></div>
	        	<ul class="sitemap_ul2">
	        		#set($num = 1)
	        		#foreach($entity in ${data.get("solution")})
	        			<li class="sitemap_li2" #if($num % 4 == 0) style="border:0" #end><a href="${entity.get("link")}">${entity.get("name")}</a></li>
	        			#set($num = $num + 1)
	        		#end
	        	</ul>
        	</li>
        	<div style="clear:both"></div>
        	<li class="sitemap_li1">
	        	<a href="/product/index/1" class="sitemap_title1">产品与平台</a>
	        	<div style="clear:both"></div>
	        	<ul class="sitemap_ul2">
	        		#set($num = 1)
	        		#foreach($entity in ${data.get("product")})
	        			<li class="sitemap_li2" #if($num % 4 == 0) style="border:0" #end><a href="${entity.get("link")}">${entity.get("name")}</a></li>
	        			#set($num = $num + 1)
	        		#end
	        	</ul>
        	</li>
        	<div style="clear:both"></div>
        	<li class="sitemap_li1">
	        	<a href="/service/index/0/1" class="sitemap_title1">服务</a>
	        	<div style="clear:both"></div>
	        	<ul class="sitemap_ul2">
	        		#set($num = 1)
	        		#foreach($type in ${data.get("service")})
		        		<li class="sitemap_li2" #if($num % 4 == 0) style="border:0" #end>
		        			<a class="sitemap_title2" href="${type.get("link")}">${type.get("name")}</a>
		        			<div style="clear:both"></div>
			        		<ul>
				        		#foreach($entity in ${type.get("service")})
				        			<li class="sitemap_li3" ><a href="${entity.get("link")}">${entity.get("name")}</a></li>
				        			
				        		#end
			        		</ul>
		        		</li>
		        		#if($num % 4 == 0) <div style="clear:both;height:20px;"></div> #end
		        		#set($num = $num + 1)
	        		 #end
	        	</ul>
        	</li>
        	<div style="clear:both"></div>
        	<li class="sitemap_li1">
	        	<a href="/news/index/0/1" class="sitemap_title1">新闻</a>
	        	<div style="clear:both"></div>
	        	<ul class="sitemap_ul2">
	        		#set($num = 1)
	        		#foreach($entity in ${data.get("news")})
	        			<li class="sitemap_li2" #if($num % 4 == 0) style="border:0" #end><a href="${entity.get("link")}">${entity.get("name")}</a></li>
	        			#set($num = $num + 1)
	        		#end
	        	</ul>
        	</li>
        	<div style="clear:both"></div>
        	<li class="sitemap_li1">
	        	<a class="sitemap_title1">公司介绍</a>
	        	<div style="clear:both"></div>
	        	<ul class="sitemap_ul2">
	        		#set($num = 1)
	        		#foreach($entity in ${data.get("companyInfo")})
	        			<li class="sitemap_li2" #if($num % 4 == 0) style="border:0" #end><a href="${entity.get("link")}">${entity.get("name")}</a></li>
	        			#set($num = $num + 1)
	        		#end
	        	</ul>
        	</li>
        	<div style="clear:both"></div>
        </ul>
    	<div style="clear:both;height:40px;"></div>
    </div>

    #parse("footer.vm")
    
</div>
<script type="text/javascript" src="/js/jquery-1.4.1.js"></script>
<script type="text/javascript" src="/js/webjs.js"></script>  
</body>
</html>