<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><meta http-equiv="Cache-control" content="private"/>
<link type="text/css" rel="stylesheet" href="/css/webcss.css"/> 
<title>中冶赛迪重庆信息技术有限公司</title>
</head>
<body>
<div class="main">
    <!--header-->
    #parse("header.vm")
    <!--左边框-->
    <div class="left_frame">
        <ul class="left_ul">
            <li class="left_title"><a href="/companyinfo/index">公司介绍</a></li>
            #foreach($entity in $data.get("names"))
            	<li #if(${entity.get("id")} == ${data.get("id")}) class="left_li_active" #else class="left_li" #end>
	                <div>
	                    <a href="${entity.get("link")}">${entity.get("name")}</a>
	                    <span> </span>
	                    <div style="clear:both;"></div>
	                </div>
	            </li>
            #end
            
        </ul>
        <div style="clear:both"></div>
    </div>
    
    
    <!--右边框-->
    <div class="right_frame">
        <div class="current_location">
            <img style="margin-left:20px;float:left;margin-top: 5px;" src="/img/dangqian.png">
            <a href="/companyinfo/index" class="location_a"><span class="location_span2">公司介绍</span></a>
            <a  class="location_a"> <span class="location_bigbig" style="margin-top: 3px;">>></span><span class="location_span2">${data.get("name")}</span></a>
            <div style="clear:both"></div>
        </div>
        <div style="clear:both"></div>
        <div class="right_content">
            <div class="right_item">
                <div class="right_item_up">
                    <img style="float:left;" src="/img/twopoint_03.png">
                    <a  style="float:left;margin-left:10px;" >${data.get("name")}</a>
                </div>
                <div class="right_item_content">${data.get("content")}
                </div>
             </div>
        </div>
        <div style="clear:both"></div>
        
    </div>
    
    <!--footer-->
   #parse("footer.vm")
</div>
<script type="text/javascript" src="/js/jquery-1.4.1.js"></script>
<script type="text/javascript" src="/js/webjs.js"></script> 
</body>
</html>