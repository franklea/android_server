    var t=setInterval(pic_normal_run,3000);
    var npic=0;
    $("#pic_list").hover(function(){clearInterval(t)}, function(){t = setInterval(pic_normal_run, 3000);});
    function pic_normal_run()
    {
        pic_scroll_left(500);
    }
    function pic_fast_left()
    {
        pic_scroll_left(100);
    }
    function pic_fast_right()
    {
        pic_scroll_right(100);
    }
    function pic_scroll_left(speed)
    {
        var _scroll = $("#pic_list_ul");
        $("#pic_list_ul").animate({"margin-left":"-920px"},speed,function(){
            _scroll.css({"margin-left":"0px"}).find("li:first").remove().appendTo(_scroll);
            npic=npic>=$("#pic_list_ul").children().length-1?0:npic+1;
            $("#pic_text li").eq(npic).css({"background":"#fff",'color':'#000'}).siblings().css({"background":"#000",'color':'#fff'});
        });
    }
    function pic_scroll_right(speed)
    {
        var _scroll = $("#pic_list_ul");
        _scroll.css({"margin-left":"-920px"}).find("li:last").remove().prependTo(_scroll);
            _scroll.animate({"margin-left":"0px"},speed,function(){
            npic=npic<0?$("#pic_list_ul").children().length-1:npic-1;
            $("#pic_text li").eq(npic).css({"background":"#fff",'color':'#000'}).siblings().css({"background":"#000",'color':'#fff'});
        });
    }
    $("#left_go").click(function(){
        pic_scroll_right(200);
    });
    $("#right_go").click(function(){
        pic_scroll_left(200);
    });
    $("#pic_text li").click(function(){
        //console.log($(this).text());
        var i = $(this).text()-1;
        if(i>npic)
        {
            for(var j=0;j<i-npic;j++)
            {
                setTimeout(pic_fast_left,120*j);
            }
        }
        else if(i < npic)
        {
            for(var j=0;j<npic-i;j++)
            { 
                setTimeout(pic_fast_right,120*j);
            }
        }
    });
    
    
    
    $(".news_li:first").show();
    var showIndex=0;
    var t2=setInterval(moveNew,3000);
    var newlis=$(".news_li");
