<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd><html><head><meta http-equiv=Content-Type content="text/html; charset=utf-8" /><meta http-equiv=Cache-control content="private"/><link type=text/css rel=stylesheet href="/css/webcss.css"/><title>中冶赛迪重庆信息技术有限公司</title></head><body><div class=main>#parse("header.vm")<div class=left_frame><ul class=left_ul><li class=left_title><a href=/news/index/0/1>新闻信息</a></li> #set($name="未知") #foreach($entity in $data.get("newsname"))<li #if(${data.get("type")}==${entity.get("id")}) class=left_li_active #set($name=${entity.get("name")}) #else class=left_li #end><div><a href="${entity.get("link")}">${entity.get("name")}</a><span></span><div style=clear:both;></div></div></li>#end</ul><div style=clear:both></div></div><div class=right_frame><div class=current_location><img style="margin-left:20px;float:left;margin-top: 5px;" src=/img/dangqian.png> <a href=/news/index/0/1 class=location_a><span class=location_span2>新闻信息</span></a> #if(${data.containsKey("type")} && ${data.get("type")} != 0) <a href="/news/index/${data.get("type")}/1" class=location_a><span class=location_bigbig style="margin-top: 3px;">>></span><span class=location_span2>$name</span></a> #end <a class=location_a href=#><span class=location_bigbig style="margin-top: 3px;">>></span><span class=location_span2>${data.get("news").get("title")}</span></a><div style=clear:both></div></div><div class=right_content><div class=right_item><div class=right_item_up><a style=float:left;margin-left:10px;>${data.get("news").get("title")}<span class=news_date>${data.get("news").get("date")}</span></a></div><div class=right_item_content>${data.get("news").get("content")}</div></div></div><div style=clear:both></div></div>#parse("footer.vm")</div><script type="text/javascript" src="/js/jquery-1.4.1.js"></script> <script type="text/javascript" src="/js/webjs.js"></script></body></html>